  "license": "MIT",
  "scripts": {
    "build:release": "yarn workspace @govtnz/ds run build:release",
    "build:website:compile-markdown": "yarn compile-markdown",
    "build:website": "yarn build",
    "build": "gatsby build && cp public/404/index.html public/error.html",
    "build__comment": "copying 404 because the host wants an error page at error.html",
    "compile-markdown": "node ./prebuild/prebuild.js",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "watch-prebuild": "nodemon ./prebuild/prebuild.js",
    "format": "prettier --write \"src/**/*.js\"",
    "ci:validate-build": "yarn jest -- --runInBand",
    "validate-build": "yarn ci:validate-build && yarn axe-test",
    "axe-test": "node postbuild/axe.js",
    "test": "yarn validate-build",
    "test:typescript": "exit 0",
    "node-modules-clear": "rimraf ./node_modules",
    "cache-clear": "rimraf ./public/ && rimraf ./.cache && rimraf .tmp && rimraf static/basics && rimraf static/components && rimraf static/css"
  },
  "nodemonConfig": {
    "ext": "tsx ts html css",
    "watch": [
      "src/commons/templates/",
      "../govt-ds/releases/"
    ],
    "delay": "2500"
  },
  "devDependencies": {}
}
