    "babel-preset-gatsby-package": "^0.4.4",
    "copy-text-to-clipboard": "^2.2.0",
    "core-js": "^3.6.5",
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
